{
	"version": "0.2.0",
	"configurations": [
	{
		"name": "TesterApp Launch",
		"type": "coreclr",
		"request": "launch",
		"preLaunchTask": "Build",
		"program": "${workspaceFolder}/src/TesterApp/bin/Debug/net5.0/TesterApp.dll",
		"cwd": "${workspaceFolder}/src/TesterApp",
		"internalConsoleOptions": "openOnSessionStart",
		"env":
		{
			"ASPNETCORE_ENVIRONMENT": "Development"
		}
	}]
}